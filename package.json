  "bugs": {
    "url": "https://github.com/helenyau0/simple-book-store/issues"
  },
  "homepage": "https://github.com/helenyau0/simple-book-store#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "nodemon": "^1.12.0",
    "pg-promise": "^6.5.1",
    "pug": "^2.0.0-rc.4"
  }
}
